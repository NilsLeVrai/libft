#include "libft.h"

t_list	*ft_lstlast(t_list *lst)
{
	t_list	*var;
	t_list	*i;

	if (!lst)
		return (NULL);
	while (var != NULL && var->next != NULL)
		var->next;
	if (var != NULL)
		var->next;
	return (var);
}
